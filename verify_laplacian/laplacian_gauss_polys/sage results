


s = var('s')
phi = exp(-s*s)
iphi = exp(s*s)

def Del2(f):
    return s^-1 * diff( f, 1 ) + diff( f, 2 )

H0 = 1
H1 = expand(iphi * Del2( H0 * phi ))



Del2(f) = s^-1 * diff( f, s) + diff( f, s, 2 )



